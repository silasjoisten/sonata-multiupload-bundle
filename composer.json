{
    "name": "silasjoisten/sonata-multiupload-bundle",
    "description": "A Bundle which allows you to enable Multiupload in Sonata",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Silas Joisten",
            "email": "silasjoisten@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "sonata-project/admin-bundle": "^3.35",
        "sonata-project/media-bundle": "^3.15"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.11",
        "matthiasnoback/symfony-dependency-injection-test": "^1.1",
        "phpunit/phpunit": "^5.6",
        "sensio/generator-bundle": "^3.1",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/webpack-encore-pack": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SilasJoisten\\Sonata\\MultiUploadBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilasJoisten\\Sonata\\MultiUploadBundle\\Tests\\": "tests/"
        }
    }
}
